<?xml version="1.0"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="script-src">
            <values>
                <value id="nf_checkout" type="host">*.nofraud-test.com</value>
                <value id="prod_nf_checkout" type="host">*.nofraud.com</value>
            </values>
        </policy>
        <policy id="frame-src">
            <values>
                <value id="nf_checkout" type="host">*.nofraud-test.com</value>
                <value id="prod_nf_checkout" type="host">*.nofraud.com</value>            
            </values>
        </policy>
        <policy id="img-src">
            <values>
                <value id="nf_checkout" type="host">*.nofraud-test.com</value>
                <value id="prod_nf_checkout" type="host">*.nofraud.com</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
